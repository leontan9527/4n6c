.table-header{padding:10px 15px;}
.hsplit{height: 8px; background-color:#fff; }
.move-container {
  width: 100%;
  min-height: 500px;
  padding-top:10px;
}

#mychart-dom-move-bar {
  width: 100%;
  height: 600px;
  margin: 0;  
  display: block;
}

.container {
  position: absolute;
  top: 80;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

ec-canvas {
  width: 100%;
  height: 500px;
}

.table-title{  
  padding: 8px 15px;
  background-color:#fff;
}

.table {
  display: table;
  margin: 8px;
  width: 96%;
  border-collapse: collapse; 
}

.table-tr {
  display: table-row; 
  height: 36px; 
}

.table-th {
  display: table-cell;    
  text-align: center;
  vertical-align: middle;
}

.table-td {
  display: table-cell;  
  text-align: center;
  vertical-align: middle;
  color: #666;
}

.bg-g{
  background: #E6F3F9;
}

.tleft{text-align: left; padding-left: 10px;}
