
.move-container {
  width: 100%;
  min-height: 600px;
}

#mychart-dom-move-bar {
  width: 100%;
  height: 600px;
  margin: 0;  
  display: block;
}

.table-title{  
  margin: 0 10px;
  display: block;
}

.table {
  display: table;
  margin: 8px;
  width: 99%;
  border-collapse: collapse; 
}

.table-tr {
  display: table-row; 
  height: 36px; 
}

.table-th {
  display: table-cell;  
  text-align: center;
  vertical-align: middle;
}

.table-td {
  display: table-cell;
  text-align: center;
  vertical-align: middle;
  color: #666;
}

.bg-g{
  background: #E6F3F9;
}

.tleft{text-align: left; padding-left: 10px;}